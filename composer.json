{
    "name": "medix-solution-suite/medix-solution-suite",
    "description": "Complete Hospital Management System",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "MedixSolutionSuite\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dibya",
            "email": "dibyakrishna@gmail.com"
        }
    ],
    "require-dev": {
        "coenjacobs/mozart": "^0.7.1"
    },
    "extra": {
        "mozart": {
            "dep_namespace": "CoenJacobs\\TestProject\\Dependencies\\",
            "dep_directory": "/mozart/Dependencies/",
            "classmap_directory":"/src/",
            "classmap_prefix": "CoenJacobs\\TestProject\\Dependencies\\",
            "packages": [
                "pimple/pimple"
            ],
            "excluded_packages": [
                "psr/container"
            ],
            "override_autoload": {
                "google/apiclient": {
                    "classmap": [
                        "src/"
                    ]
                }
            },
            "delete_vendor_directories": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\"vendor/bin/mozart\" compose",
            "php composer.phar dump-autoload"
        ],
        "post-update-cmd": [
            "\"vendor/bin/mozart\" compose",
            "php composer.phar dump-autoload"
        ]
    },
    "require": {
        "pimple/pimple": "^3.5"
    }
}
